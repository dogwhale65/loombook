{
	"schemaVersion": 1,
	"id": "loombook",
	"version": "${version}",
	"name": "loombook",
	"description": "Mod that adds a side panel to the loom screen to save and load patterns",
	"authors": [
		"dogwhale65"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/loombook/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dogwhale65.loombook.Loombook"
		],
		"client": [
			"dogwhale65.loombook.LoombookClient"
		]
	},
	"mixins": [
		"loombook.mixins.json",
		{
			"config": "loombook.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.6",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}